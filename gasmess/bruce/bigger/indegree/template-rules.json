    "flourishing": {
      "reference": "Flourishing (Diener-8) [Validated Scale]",
      "type": "measure_set",
      "concept": "Psychological flourishing (meaning, relationships, engagement, optimism, purpose).",
      "description": "Measures meaning, relationships, engagement, optimism, and purpose.",
      "sections": [
        {
          "title": "Flourishing",
          "items": [
            {
              "questionType": "multiple_choice_grid",
              "required": false,
              "title": "Below are 8 statements with which you may agree or disagree. Using the 1-7 scale below, indicate your agreement with each item by indicating that response for each statement.",
              "labels": {
                "Strongly disagree": 1,
                "Disagree": 2,
                "Slightly disagree": 3,
                "Mixed or neither agree nor disagree": 4,
                "Slightly agree": 5,
                "Agree": 6,
                "Strongly agree": 7
              },
              "questions": [
                {
                  "id": "fl_competent",
                  "text": "I am competent and capable in the activities that are important to me."
                },
                {
                  "id": "fl_good",
                  "text": "I am a good person and live a good life."
                },
                {
                  "id": "fl_optimistic",
                  "text": "I am optimistic about my future."
                },
                {
                  "id": "fl_respect",
                  "text": "People respect me."
                },
                {
                  "id": "fl_meaningful",
                  "text": "I lead a purposeful and meaningful life."
                },
                {
                  "id": "fl_rewarding",
                  "text": "My social relationships are supportive and rewarding."
                },
                {
                  "id": "fl_engaged",
                  "text": "I am engaged and interested in my daily activities."
                },
                {
                  "id": "fl_contribute",
                  "text": "I actively contribute to the happiness and well-being of others."
                }
              ]
            }
          ]
        }
      ]
    },
    "happiness": {
      "reference": "Happiness (Subjective Happiness Scale-4) [Validated Scale]",
      "type": "measure_set",
      "concept": "Subjective happiness / global life satisfaction.",
      "description": "Subjective happiness / global life satisfaction.",
      "sections": [
        {
          "title": "For each of the following statements and/or questions, please mark the point on the scale that you feel is most appropriate in describing you.",
          "items": [
            {
              "questionType": "linear_scale",
              "required": false,
              "labels": {
                "1": 1,
                "7": 7
              },
              "questions": [
                {
                  "id": "hs_consider",
                  "text": "In general, I consider myself:"
                },
                {
                  "id": "hs_compare",
                  "text": "Compared to most of my peers, I consider myself:"
                },
                {
                  "id": "hs_general",
                  "text": "Some people are generally very happy. They enjoy life regardless of what is going on, getting the most out of everything. To what extent does this characterization describe you?"
                },
                {
                  "id": "hs_not_general",
                  "text": "Some people are generally not very happy. Although they are not depressed, they never seem as happy as they might be. To what extent does this characterization describe you?"
                }
              ]
            }
          ]
        }
      ]
    },
